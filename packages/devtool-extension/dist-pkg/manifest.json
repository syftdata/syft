{
  "name": "Syft Studio",
  "description": "Record browser interactions to auto generate event tests, and auto-magically instrument application code.",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "contextMenus",
    "webNavigation",
    "debugger"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "32": "img/logo-32.png",
    "48": "img/logo-48.png",
    "128": "img/logo-128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader.d6c33073.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/chunk-8740526c.js",
        "assets/chunk-4ac3b015.js"
      ],
      "use_dynamic_url": true
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/chunk-2d8509c2.js",
        "assets/chunk-e266c12f.js",
        "assets/chunk-8740526c.js",
        "assets/chunk-6ed47567.js",
        "assets/index.2ab409f4.css"
      ],
      "use_dynamic_url": false
    }
  ]
}

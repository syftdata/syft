{
  "name": "Syft Studio",
  "description": "Syft Studio lets you record, test and instrument your web apps.",
  "version": "0.0.3",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "contextMenus",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "32": "img/logo-32.png",
    "48": "img/logo-48.png",
    "128": "img/logo-128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader.e7a6c7e8.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "assets/devtools_source.ts-loader.98ecd687.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/chunk-8ba139f3.js",
        "assets/chunk-cbbc8866.js",
        "assets/chunk-4630ab10.js"
      ],
      "use_dynamic_url": true
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/chunk-34b209c4.js",
        "assets/chunk-058300ef.js",
        "assets/chunk-8ba139f3.js",
        "assets/chunk-5dd9aef1.js",
        "assets/index.2ab409f4.css"
      ],
      "use_dynamic_url": false
    }
  ]
}
